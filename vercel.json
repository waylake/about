{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/blog/:slug*",
      "destination": "/api/server.js"
    },
    {
      "source": "/about",
      "destination": "/api/server.js"
    },
    {
      "source": "/projects",
      "destination": "/api/server.js"
    },
    {
      "source": "/contact",
      "destination": "/api/server.js"
    },
    {
      "source": "/blog",
      "destination": "/api/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/server.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "regions": ["icn1"]
}
